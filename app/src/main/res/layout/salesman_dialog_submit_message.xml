<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/highlights"
    android:gravity="center">

    <LinearLayout
        android:layout_width="300dp"
        android:layout_height="400dp"
        android:gravity="center"
        android:orientation="vertical">
        <!-- app:srcCompat="@drawable/succesfully"-->
        <ImageView
            android:id="@+id/order_dialog_credit_note_images"
            android:layout_width="170dp"
            android:layout_height="100dp"
            app:srcCompat="@drawable/ic_thum_success"/>

        <com.app.entero.direct.utils.custom.CustomTextView_Salesman
            android:id="@+id/text_dialog_credit_note_submit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:text="Credit note has been submited succesfully?"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="@color/green"
            android:textStyle="bold" />
    </LinearLayout>
</LinearLayout>
